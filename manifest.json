{
    "name": "Productivity Chestnut",
    "version": "1.0.0",
    "description": "Study Partner",
    "manifest_version": 3,
    "author": "Thomas Li",
    "action": {
        "default_popup": "./popup/popup.html",
        "default_title": "Chestnut"
    },
    "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],

  "background": {
    "service_worker": [
      "src/blocker.js"
    ]
  }

}


